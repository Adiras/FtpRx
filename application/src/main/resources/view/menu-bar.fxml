<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ftprx.application.controller.MenuBarController">
   <menus>
      <Menu mnemonicParsing="false" text="File">
         <items>
            <SeparatorMenuItem mnemonicParsing="false" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Help">
         <items>
            <MenuItem mnemonicParsing="false" text="Check for updates" />
            <MenuItem mnemonicParsing="false" onAction="#onAboutButtonClick" text="About" />
         </items>
      </Menu>
   </menus>
</MenuBar>
