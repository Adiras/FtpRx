<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<ToolBar prefHeight="60.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ftprx.application.controller.ToolBarController">
   <items>
      <Button fx:id="startButton" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#onStartButtonClick" prefHeight="49.0" prefWidth="49.0" text="Start">
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/next.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="stopButton" contentDisplay="TOP" disable="true" focusTraversable="false" mnemonicParsing="false" onAction="#onStopButtonClick" prefHeight="49.0" prefWidth="49.0" text="Stop">
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/abort.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="pauseButton" contentDisplay="TOP" disable="true" focusTraversable="false" mnemonicParsing="false" onAction="#onPauseButtonClick" prefHeight="49.0" prefWidth="49.0" text="Pause">
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/pause.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
      <Button fx:id="userManagerButton" contentDisplay="TOP" focusTraversable="false" mnemonicParsing="false" onAction="#onUserManagerButtonClick" prefHeight="49.0" prefWidth="49.0" text="Users">
         <graphic>
            <ImageView fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/people.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </items>
</ToolBar>
